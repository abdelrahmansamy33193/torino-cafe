{
  "name": "torino-cafe",
  "version": "1.0.0",
  "description": "React + Webpack setup for Torino Cafe",
  "main": "src/index.js",
  "scripts": {
  "start": "cross-env NODE_ENV=development BABEL_ENV=development webpack serve --config webpack.dev.js",
  "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --config webpack.prod.js",
  "preview": "serve -s dist -l 5000",
  "clean": "rimraf dist node_modules/.cache",
  "rebuild": "npm run clean && npm run build"
}
,
  "keywords": [
    "react",
    "webpack",
    "babel",
    "torino"
  ],
  "author": "Abd-Elrahman",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.38.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
